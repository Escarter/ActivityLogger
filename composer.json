{
    "name": "clebea/activitylogger",
    "description": "This logs app activities in the database",
    "license": "MIT",
    "authors": [
        {
            "name": "Escarter Brieno",
            "email": "brienom@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.0",
        "illuminate/contracts": "^5.1",
        "illuminate/support": "^5.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "CleBea\\ActivityLogger\\": "src/"
        },
        "classmap": [
            "database",
            "clebea/activitylogger/src/database/migrations"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        },
        "laravel": {
            "providers": [
                "CleBea\\ActivityLogger\\ActivityLoggerServiceProvider"
            ],
            "aliases": {
                "ActivityLogger": "CleBea\\ActivityLogger\\Facade\\"
            }
        }

    }
}
